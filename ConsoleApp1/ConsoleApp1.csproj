	</PropertyGroup>


	<ItemGroup>
		<PackageReference Include="AngleSharp" Version="1.2.0-beta.431" />
		<PackageReference Include="AngleSharp.Io" Version="1.0.0" />
		<PackageReference Include="AngleSharp.Js" Version="1.0.0-beta.36" />
